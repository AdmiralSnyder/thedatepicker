{
	"compilerOptions": {
		"target": "ES5",
		"module": "CommonJS",
		//"strict": true,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"rootDir": "./src/",
		"declaration": true,
		"declarationDir": "dist",
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"],
	"exclude": ["dist"]
}
